              <rect width="64" height="64" fill="url(#pattern0_15_2772)"/>
              <defs>
              <pattern id="pattern0_15_2772" patternContentUnits="objectBoundingBox" width="1" height="1">
              <use xlink:href="#image0_15_2772" transform="scale(0.0147059)"/>
              </pattern>
              <image id="image0_15_2772" width="68" height="68" xlink:href="data:image/png;base64,"/>
              </defs>
            </svg>
          </button>
          <label for="filter-random" class="filter-random">Random</label>
        </a>
      </div>

      <form action="/" id="dishForm" class="desktop-hidden">

        <fieldset class="fieldset-filter-dish">
          <legend class="visually-hidden">Filter op gerecht</legend>
          <select id="filter-dish" class="filter-dish" aria-labelledby="filter-label">
            <option value="">Gerecht</option>
            {% for dish in dishes %}
              <option value="{{ dish.fav_kitchen }}">
                {{ dish.fav_kitchen }}
              </option>
            {% endfor %}
          </select>
          <label id="filter-dish-label" class="filter-dish-label">Gerecht</label>
        </fieldset>
      </form>
