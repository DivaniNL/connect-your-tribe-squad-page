      {% if dishes.size  > 0 %}
        <input type="checkbox" name="filter-dish-list-toggler" class="filter-dish-list-toggler mobile-hidden" aria-label="Toon gerechten" id="filter-dish-list-toggler" data-selected-dish="Gerecht-Filter: Geen" data-selected-dish-short="Gerecht">
        <label for="filter-dish-list-toggler" class="mobile-hidden">Kies een gerecht</label>
      {% endif %}
      {% if dishes.size > 0 %}
        <ul class="filter-dish-list mobile-hidden">
          {% for dish in dishes %}
            <li class="filter-dish-list-item">
              <a href="/gerecht/{{ dish.fav_kitchen }}">
                <img src="/assets/img/dishes/{{ dish.fav_kitchen }}.jpg" alt="afbeelding van {{ dish.fav_kitchen }}" class="filter-dish-list-item-image">
                <span aria-hidden="true">{{ dish.fav_kitchen }}</span>
              <span class="visually-hidden">Filter op {{ dish.fav_kitchen }}</span>
              </a>
            </li>
          {% endfor %}
          <li class="filter-dish-list-item">
            <a href="/gerecht/">
              <img src="/assets/img/dishes/Reset.jpg" alt="Reset Filter" class="filter-dish-list-item-image">
              Reset Filter
            </a>
        </ul>
      {% endif %}
      <div class="button-container button-form-container button-form-container-random">
        <a href="/random" class="button-form-container-random-link">
          <button type="button" class="button-form button-form-random" id="filter-random" aria-label="Wissel tussen random en normale modus. Dit is nu de Normale modus">
